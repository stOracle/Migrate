import random
def main():
    
    hand = [(4,'h'), (3,'s')]
    score = hand[0][0] + hand[1][0]
    print("hand = ", hand)
    print()
    for i in hand:
        print(hand[i])
    
    print("hand[0] = ", hand[0])
    print("hand[0][0] = ", hand[0][0])
    print(score)
    
    
    
main()